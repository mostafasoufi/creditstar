{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "id",
      "first_name",
      "last_name",
      "email",
      "personal_code",
      "phone",
      "active",
      "dead",
      "lang"
    ],
    "properties": {
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "7521"
        ],
        "pattern": "^(.*)$"
      },
      "first_name": {
        "$id": "#/items/properties/first_name",
        "type": "string",
        "title": "The First_name Schema",
        "default": "",
        "examples": [
          "Ullalaa"
        ],
        "pattern": "^(.*)$"
      },
      "last_name": {
        "$id": "#/items/properties/last_name",
        "type": "string",
        "title": "The Last_name Schema",
        "default": "",
        "examples": [
          "Ziip"
        ],
        "pattern": "^(.*)$"
      },
      "email": {
        "$id": "#/items/properties/email",
        "type": "string",
        "title": "The Email Schema",
        "default": "",
        "examples": [
          "ulallaaa@mldllm.tt"
        ],
        "pattern": "^(.*)$"
      },
      "personal_code": {
        "$id": "#/items/properties/personal_code",
        "type": "string",
        "title": "The Personal_code Schema",
        "default": "",
        "examples": [
          "49005025465"
        ],
        "pattern": "^(.*)$"
      },
      "phone": {
        "$id": "#/items/properties/phone",
        "type": "string",
        "title": "The Phone Schema",
        "default": "",
        "examples": [
          "50170262"
        ],
        "pattern": "^(.*)$"
      },
      "active": {
        "$id": "#/items/properties/active",
        "type": "string",
        "title": "The Active Schema",
        "default": "",
        "examples": [
          "1"
        ],
        "pattern": "^(.*)$"
      },
      "dead": {
        "$id": "#/items/properties/dead",
        "type": "string",
        "title": "The Dead Schema",
        "default": "",
        "examples": [
          "0"
        ],
        "pattern": "^(.*)$"
      },
      "lang": {
        "$id": "#/items/properties/lang",
        "type": "string",
        "title": "The Lang Schema",
        "default": "",
        "examples": [
          "est"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}